	rm -f $(NAME) bonus
	make -C $(LIBFT_PATH) fclean
	make -C $(LIBFT_PATH) clean
	make -C $(LIBFT_PATH)
	$(CC) $(CFLAGS) -o $@ $(OBJCS) $(LIBFT_PATH)/libft.a
